#!/usr/bin/with-contenv sh

set -euo pipefail
export BF_E="${PWD##*/}/$(basename ${0})"


#======================================================================================================================
# Forward cron log to Docker.
#======================================================================================================================

[[ ! -f ${CRON_LOG_FILE} ]] && touch ${CRON_LOG_FILE}
bf-forward ${PWD##*/} ${CRON_LOG_FILE}
